{
  "name": "webviewer-html-annotate-proxy",
  "version": "1.0.0",
  "description": "Annotate live HTML pages by simply providing URL",
  "scripts": {
    "start-server": "npm start --prefix server",
    "start-client": "npm start --prefix client",
    "start": "concurrently \"npm run start-client\" \"npm run start-server\" -n \"CLIENT,SERVER\" -c \"bgBlueBright.whiteBright,bgMagenta.whiteBright\"",
    "install-server": "npm install --prefix server",
    "install-client": "npm install --prefix client",
    "setup": "npm i && concurrently \"npm run install-client\" \"npm run install-server\" -n \"CLIENT,SERVER\" -c \"bgBlueBright.whiteBright,bgMagenta.whiteBright\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XodoDocs/webviewer-html-annotate-proxy.git"
  },
  "author": "Maggie Vu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/XodoDocs/webviewer-html-annotate-proxy/issues"
  },
  "homepage": "https://github.com/XodoDocs/webviewer-html-annotate-proxy#readme",
  "dependencies": {
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0"
  }
}
